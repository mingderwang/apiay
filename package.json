{
  "name": "apiay",
  "version": "0.0.1",
  "description": "protocol to pay api per call by crypto",
  "source": "src/main.jsx",
  "main": "dist/main.umd.js",
  "module": "dist/main.modern.module.js",
  "unpkg": "dist/main.umd.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "bin": {
    "daapy": "src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mingderwang/apiay.git"
  },
  "scripts": {
    "build:test": "node ./scripts/build-test.js",
    "build": "microbundle --define PKG_VERSION=$(npm show . version)",
    "watch": "microbundle watch --define PKG_VERSION=$(npm show . version)",
    "build:react": "yarn clean && node ./scripts/build.js",
    "build:node": "yarn clean && node ./scripts/build-node.js",
    "start": "node ./dist/app.js",
    "test": "jest",
    "test:ava": "ava",
    "clean": "rimraf ./dist",
    "release": "np"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ],
  "keywords": [
    "template",
    "typescript",
    "microbundle",
    "esbuild",
    "ava"
  ],
  "dependencies": {
    "preact": "^10.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "ava": "^4.3.1",
    "esbuild-jest": "^0.5.0",
    "esbuild-wasm": "^0.14.51",
    "jest": "^28.1.3",
    "microbundle": "^0.15.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts": "^0.2.2",
    "ts-node": "^10.9.1",
    "xo": "^0.51.0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--trace-warnings",
      "--loader=ts-node/esm"
    ]
  },
  "author": {
    "name": "Ming-der Wang",
    "email": "mingderwang@gmail.com",
    "url": "http://mingderwang.tumblr.com/"
  }
}
